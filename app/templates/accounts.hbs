<h2>Accounts</h2>
<table>
  <thead>
    <th>Email Address</th>
    <th>Display Name</th>
    <th>Status</th>
    <th>Last Login</th>
    <th>Description</th>
  </thead>
  
  <tbody>
    {{#each this}}
    <tr>
      <td>{{#link-to "account" this}}{{name}}{{/link-to}}</td>
      <td>{{displayName}}</td>
      <td>{{zimbraAccountStatus}}</td>
      <td>{{moment-format zimbraLastLogonTimestamp format="fromNow" defaultValue="Never logged in"}}</td>
      <td>{{description}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>
