<ol class="breadcrumb">
  <li>{{#link-to "index"}}Home{{/link-to}}</li>
  <li class="active">Zimlets</li>
</ol>

<table>
  <thead>
    <th>&nbsp;</th>
    <th>Name</th>
    <th>Display Name</th>
    <th>Description</th>
    <th>Version</th>
    <th>Status</th>
  </thead>
  
  <tbody>
    {{#each this}}
    <tr>
      <td><span class="glyphicon glyphicon-link"/>
      <td>{{name}}</td>
      <td>{{displayName}}</td>
      <td>{{description}}</td>
      <td>{{version}}</td>
      <td>{{#if enabled}}Enabled{{else}}Disabled{{/if}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>
