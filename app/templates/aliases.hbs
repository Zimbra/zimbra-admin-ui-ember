<ol class="breadcrumb">
  <li>{{#link-to "index"}}Home{{/link-to}}</li>
  <li class="active">Aliases</li>
</ol>

<table>
  <thead>
    <th>Email Address</th>
    <th>Target Name</th>
    <th>Target Type</th>
    <th>Description</th>
  </thead>
  
  <tbody>
    {{#each this}}
    <tr>
      <td>{{name}}</td>
      <td>{{#link-to "account" zimbraAliasTargetId}}{{targetName}}{{/link-to}}</td>
      <td>{{type}}</td>
      <td>{{description}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>
