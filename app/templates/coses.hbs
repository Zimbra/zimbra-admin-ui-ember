<ol class="breadcrumb">
  <li>{{#link-to "index"}}Home{{/link-to}}</li>
  <li class="active">Classes of Service</li>
</ol>

<table>
  <thead>
    <th>&nbsp;</th>
    <th>Name</th>
    <th>Description</th>
  </thead>
  
  <tbody>
    {{#each this}}
    <tr>
      <td><span class="glyphicon glyphicon-usd"/>
      <td>{{name}}</td>
      <td>{{description}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>
