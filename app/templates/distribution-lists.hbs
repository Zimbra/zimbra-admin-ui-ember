<ol class="breadcrumb">
  <li>{{#link-to "index"}}Home{{/link-to}}</li>
  <li class="active">Distribution Lists</li>
</ol>

<table>
  <thead>
    <th>&nbsp;</th>
    <th>Email Address</th>
    <th>Display Name</th>
    <th>Status</th>
    <th>Last Login</th>
    <th>Description</th>
  </thead>
  
  <tbody>
    {{#each dl in model}}
    <tr>
      <td><span class="glyphicon glyphicon-th-list"/></td>
      <td>{{dl.name}}</td>
      <td>{{dl.displayName}}</td>
      <td>{{dl.zimbraMailStatus}}</td>
      <td>{{moment-format dl.zimbraLastLogonTimestamp format="fromNow" defaultValue="Never logged in"}}</td>
      <td>{{dl.description}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>
