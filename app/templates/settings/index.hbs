{{partial "settings/-breadcrumbs"}}
{{partial "settings/-pills"}}

<br/>

{{#if searchFor}}
<div class="alert alert-info" role="alert">
  <span class="badge">{{searchResult.length}}</span> attributes match your search.
</div>
{{/if}}

<table>
  <thead>
    <th>&nbsp;</th>
    <th>Key</th>
    <th>Value</th>
  </thead>
  
  <tbody>
    {{#each setting in searchResult}}
    <tr>
      <td><span class="glyphicon glyphicon-cog"/></td>
      <td>{{unbound setting.id}}</td>
      <td>{{unbound setting.value}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>
